# Docker ignore file for MoneyMosaic
# Ignore node_modules (they will be installed in container)
node_modules/
frontend/node_modules/

# Ignore build artifacts
dist/
frontend/dist/
coverage/

# Ignore development files
.env.local
.env.development.local
.env.test.local
.env.production.local

# Ignore logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
logs/

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore editor files
.vscode/
.idea/
*.swp
*.swo
*~

# Ignore git
.git/
.gitignore

# Ignore documentation that's not needed in container
README.md
docs/

# Ignore test files
tests/
__tests__/
*.test.*
*.spec.*

# Ignore Docker files themselves
Dockerfile*
docker-compose*.yml
.dockerignore

# Ignore SQLite database files (will be created in container)
*.db
*.sqlite
*.sqlite3

# Ignore collections and other dev tools
collections/
postman_collection.json
